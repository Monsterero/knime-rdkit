<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.molecule2rdkit.Molecule2RDKitConverterNodeFactory"
            id="org.rdkit.knime.nodes.molecule2rdkit.Molecule2RDKitConverterNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.molecule2rdkit.Molecule2RDKitConverterNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.rdkit2molecule.RDKit2MoleculeConverterNodeFactory"
            id="org.rdkit.knime.nodes.rdkit2molecule.RDKit2MoleculeConverterNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.rdkit2molecule.RDKit2MoleculeConverterNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.canonsmiles.RDKitCanonicalSmilesNodeFactory"
            id="org.rdkit.knime.nodes.canonsmiles.RDKitCanonicalSmilesNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.canonsmiles.RDKitCanonicalSmilesNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.addcoordinates.RDKitAddCoordinatesNodeFactory"
            id="org.rdkit.knime.nodes.addcoordinates.RDKitAddCoordinatesNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.addcoordinates.RDKitAddCoordinatesNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.rdkfingerprint.RDKitFingerprintNodeFactory"
            id="org.rdkit.knime.nodes.rdkfingerprint.RDKitFingerprintNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.rdkfingerprint.RDKitFingerprintNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.substructfilter.RDKitSubstructFilterNodeFactory"
            id="org.rdkit.knime.nodes.substructfilter.RDKitSubstructFilterNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.substructfilter.RDKitSubstructFilterFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.moleculesubstructfilter.RDKitMoleculeSubstructFilterNodeFactory"
            id="org.rdkit.knime.nodes.moleculesubstructfilter.RDKitMoleculeSubstructFilterNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.moleculesubstructfilter.RDKitMoleculeSubstructFilterNodeFactory"
            category-path="/community/rdkit"
            expert-flag="false"
            factory-class="org.rdkit.knime.nodes.multiplesubstrucfilter.RDKitDictSubstructFilterNodeFactory"
            id="org.rdkit.knime.nodes.multiplesubstrucfilter.RDKitDictSubstructFilterNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.multiplesubstrucfilter.RDKitDictSubstructFilterNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.onecomponentreaction2.RDKitOneComponentReactionNodeFactory"
            id="org.rdkit.knime.nodes.onecomponentreaction2.RDKitOneComponentReactionNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.onecomponentreaction.RDKitOneComponentReactionNodeFactory"
            category-path="/community/rdkit"
            factory-class="org.rdkit.knime.nodes.twocomponentreaction2.RDKitTwoComponentReactionNodeFactory"
            id="org.rdkit.knime.nodes.twocomponentreaction2.RDKitTwoComponentReactionNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.canonsmiles.RDKitCanonicalSmilesNodeFactory"
            category-path="/community/rdkit/experimental"
            factory-class="org.rdkit.knime.nodes.murckoscaffold.RDKitMurckoScaffoldNodeFactory"
            id="org.rdkit.knime.nodes.murckoscaffold.RDKitMurckoScaffoldNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.murckoscaffold.RDKitMurckoScaffoldNodeFactory"
            category-path="/community/rdkit/experimental"
            factory-class="org.rdkit.knime.nodes.rgroups.RDKitRGroupsNodeFactory"
            id="org.rdkit.knime.nodes.rgroups.RDKitRGroupsNodeNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.rgroups.RDKitRGroupsNodeNodeFactory"
            category-path="/community/rdkit/experimental"
            factory-class="org.rdkit.knime.nodes.molfragmenter.RDKitMolFragmenterNodeFactory"
            id="org.rdkit.knime.nodes.molfragmenter.RDKitMolFragmenterNodeFactory"/>
      <node
            after="org.rdkit.knime.nodes.rgroups.RDKitMolFragmenterNodeFactory"
            category-path="/community/rdkit/experimental"
            factory-class="org.rdkit.knime.nodes.diversitypicker.RDKitDiversityPickerNodeFactory"
            id="org.rdkit.knime.nodes..diversitypicker.RDKitDiversityPickerNodeFactory"/>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Nodes using RDKit"
            icon="icons/category_rdkit.png"
            level-id="rdkit"
            name="RDKit"
            path="/community">
      </category>
      <category
            description="Experimental nodes"
            icon="icons/category_rdkit.png"
            level-id="experimental"
            name="Experimental"
            path="/community/rdkit">
      </category>
   </extension>
   <extension
         point="org.knime.product.splashExtension">
      <splashExtension
            icon="icons/rdkit_splash.png"
            id="rdkit"
            tooltip="RDKit">
      </splashExtension>
   </extension>
   
</plugin>
